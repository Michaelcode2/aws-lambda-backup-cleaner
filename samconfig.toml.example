version = 0.1

[default]
[default.deploy]
[default.deploy.parameters]
stack_name = "backup-cleaner-dev"
s3_bucket = "YOUR_BUCKET_NAME"
s3_prefix = "backup-cleaner-dev"
region = "YOUR_REGION"
